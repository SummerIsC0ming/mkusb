label LIVEUSB
iso 'Xubuntu 16.10' xubuntu-16.10-desktop-amd64.iso /casper/vmlinuz.efi /casper/initrd.lz 'file=/cdrom/preseed/xubuntu.seed boot=casper quiet splash iso-scan/filename=%filename%'
iso 'Fedora Workstation 25' Fedora-Workstation-Live-x86_64-25-1.3.iso /images/pxeboot/vmlinuz /images/pxeboot/initrd.img 'root=live:CDLABEL=Fedora-WS-Live-25-1-3 rd.live.image quiet iso-scan/filename=%filename%'
iso 'Tails 2.10' tails-i386-2.10.iso /live/vmlinuz2 /live/initrd2.img 'boot=live config live-media=removable apparmor=1 security=apparmor nopersistence noprompt timezone=Etc/UTC block.events_dfl_poll_msecs=1000 splash noautologin module=Tails kaslr slab_nomerge slub_debug=FZ mce=0 vsyscall=none quiet findiso=%filename%'
iso 'System Rescue CD 4.9.2' systemrescuecd-x86-4.9.2.iso /isolinux/rescue64 /isolinux/initram.igz 'docache isoloop=%filename%'
